{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run --silent=false --reporter verbose",
    "test:bench": "vitest bench",
    "test:type": "vitest typecheck",
    "coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./drizzle.config.ts",
    "auth:generate": "npx @better-auth/cli generate --config server/utils/auth --output server/database/auth-schema.ts"
  },
  "dependencies": {
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "^3.3.3",
    "@nuxtjs/i18n": "^10.1.0",
    "@vueuse/nuxt": "13.9.0",
    "better-auth": "^1.3.9",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "nuxt": "^4.1.2",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "resend": "^6.1.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.68",
    "@nuxt/eslint": "^1.9.0",
    "@types/pg": "^8.15.5",
    "@vitest/coverage-v8": "3.2.4",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "terser": "^5.44.0",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
